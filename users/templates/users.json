{
  "type": "FeatureCollection",
  "features": [ {% for user in users %}
    {
      "type": "Feature",
      "properties": {
        "name": "{{ user.name }}",
        "popupContent": "{{ user.name }}"
      },
      "geometry": {
        "type": "Point",
        "coordinates": [{{ user.longitude }}, {{ user.latitude }}]
      }
    }{% if not loop.last %},{% endif %}{% endfor %}
  ]
}
