{
  "type": "FeatureCollection",
  "features": [ {% for user in users %}
    {
      "type": "Feature",
      "properties": {
        "name": "{{ user.name }}",
        {% if user.website != "" %}
          "popupContent": "<span class='glyphicon glyphicon-user'></span> {{ user.name }} </br><span class='glyphicon glyphicon-home'></span> <a href='http://{{ user.website }}' target='_blank'>Website</a>"
        {% else %}
          "popupContent": "<span class='glyphicon glyphicon-user'></span> {{ user.name }}"
        {% endif %}
      },
      "geometry": {
        "type": "Point",
        "coordinates": [{{ user.longitude }}, {{ user.latitude }}]
      }
    }{% if not loop.last %},{% endif %}{% endfor %}
  ]
}
